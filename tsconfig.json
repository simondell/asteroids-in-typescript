{
"compilerOptions": {
  "esModuleInterop": true,
  "module": "ESNext",
  "moduleResolution": "node",
  "noImplicitAny": true,
  "removeComments": false,
  "preserveConstEnums": true,
  "outDir": "./build",
  "inlineSourceMap": true,
  "inlineSources": true
},
"include": ["src/**/*"],
"exclude": ["node_modules", "**/*.spec.ts", "**/*.test.ts"]
}